<?xml version="1.0" encoding="UTF-8"?>
<!--
   Document   : isp_payment_requests.xml
   Author     : @xfirepc (https://xfirepc.com)
   Description: Estructura de la tabla isp_payment_requests
 -->
<table>
    <column>
        <name>id</name>
        <type>serial</type>
        <null>NO</null>
    </column>
    <constraint>
        <name>sf_isp_payment_requests_primary_key</name>
        <type>PRIMARY KEY (id)</type>
    </constraint>
    <column>
        <name>idcontacto</name>
        <type>integer</type>
    </column>
    <column>
        <name>idplan</name>
        <type>integer</type>
    </column>
    <column>
        <name>total</name>
        <type>double</type>
    </column>
    <column>
        <name>date</name>
        <type>datetime</type>
    </column>
    <constraint>
        <name>ca_sfi_planes_idplan_requests</name>
        <type>FOREIGN KEY (idplan) REFERENCES sfi_planes(id) ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint>
    <constraint>
        <name>ca_sfi_contactos_idcontacto_requests</name>
        <type>FOREIGN KEY (idcontacto) REFERENCES contactos(idcontacto) ON DELETE CASCADE ON UPDATE CASCADE</type>
    </constraint>
</table>